        },
        "expectation": {
          "values":{
            "email": "test@test.com"
          }
        }
      },
      {
        "comment": "normal:email length:31",
        "body_params": {
          "email": "testtesttesttesttestte@test.com"
        },
        "expectation": {
          "values":{
            "email": "test@test.com"
          }
        }
      },
      {
        "comment": "normal:email length:32",
        "body_params": {
          "email": "testtesttesttesttesttes@test.com"
        },
        "expectation": {
          "values":{
            "email": "test@test.com"
          }
        }
      }
    ]
  }
