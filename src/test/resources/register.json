{
  "testEmailFormat": {
    "default": {
      "uri":"http://localhost:10726/token",
      "head_params":{
        "Content-Type": "application/json",
        "Accept": "application/json"
      },
      "body_params": {
        "password": "test123",
        "displayName": "testName"
      },
      "expectation":{
        "status_code": 201,
        "values":{
          "password": "test123",
          "displayName": "testName",
          "age": null,
          "gender": null
        }
      }
    },
    "cases": [
